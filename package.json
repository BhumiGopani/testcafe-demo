{
  "name": "testcafe-demo",
  "version": "1.0.0",
  "description": "Project to demo TestCafe as framework",
  "engines": {
    "node": "12.13.0"
  },
  "main": "index.js",
  "scripts": {
    "testcafe": "./node_modules/.bin/testcafe",
    "test": "npm run testcafe chrome tests/",
    "all": "npm run testcafe -- -c 2 chrome:headless,firefox:headless tests/",
    "cli": "npm run testcafe -- -c 2 chrome:headless tests/",
    "safari": "npm run testcafe safari tests/",
    "firefox": "npm run testcafe firefox tests/",
    "browsers": "npm run testcafe -- -b browserstack",
    "ie": "npm run testcafe 'browserstack:ie@11.0:Windows 10' tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BhumiGopani/testcafe-demo.git"
  },
  "keywords": [
    "TestCafe",
    "Automation",
    "Testing"
  ],
  "author": "Bhumi Gopani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BhumiGopani/testcafe-demo/issues"
  },
  "homepage": "https://github.com/BhumiGopani/testcafe-demo#readme",
  "dependencies": {
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.7",
    "package.json": "^2.0.1",
    "testcafe": "^1.8.2",
    "testcafe-browser-provider-browserstack": "^1.8.0"
  }
}
